- courses.forEach(function(course) {
    div.course
        ul.main-info
            li.course-code= course.code
            li.course-name= course.name
        p.course-description= 'Description: ' + course.description
        ul.meta
            li#course-subject.button.bg-dark-blue= 'Subject: '.toUpperCase() + course.subject.name + ' (' + course.subject.code + ')'
            li#course-credits.button.bg-dark-blue= 'Credits: '.toUpperCase() + course.credits
        ul.score
            li#take.button.bg-green= 'take: 5'.toUpperCase()
            li#pass.button.bg-green= 'pass: 3'.toUpperCase()
            li#easy.button.bg-green= 'easy: 7'.toUpperCase()
            li#hard.button.bg-green= 'hard: 3'.toUpperCase()
            li#fun.button.bg-green= 'fun: 1'.toUpperCase()
            li#useful.button.bg-green= 'useful / interesting: 4'.toUpperCase()
            li#prof.button.bg-green= 'poor profs: 0'.toUpperCase()
            li#ta.button.bg-green= 'poor TAs: 0'.toUpperCase()
            //li#grade.button.bg-green= 'grades received: A B C D E F'.toUpperCase()
- })
